<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button4"
        android:layout_width="146dp"
        android:layout_height="79dp"
        android:fontFamily="sans-serif"
        android:text="Guia Prático Horta no Apartamento"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button"
        tools:layout_editor_absoluteY="515dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="146dp"
        android:layout_height="79dp"
        android:fontFamily="sans-serif"
        android:textSize="12dp"
        android:text="Guias Práticos Agricultura Regenerativa"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="515dp"
        android:colo="@color/forest_green" />

    <Button
        android:id="@+id/button5"
        android:layout_width="146dp"
        android:layout_height="79dp"
        android:fontFamily="sans-serif"
        android:text="Informação\nIndividual\nPlanta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="634dp" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="312dp"
        android:layout_height="296dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="105dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="345dp"
        android:layout_marginEnd="31dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_menu"
        tools:layout_editor_absoluteY="16dp" />
</androidx.constraintlayout.widget.ConstraintLayout>